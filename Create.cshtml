@page
@model KT10ADO.NET.CreateModel
@{
	ViewData["Title"] = "Create User";
}
<h2>Create User</h2>

<form asp-action="Create" method="post">
    <div>
        <label>Name</label>
        <input type="text" name="name" required />
    </div>
    <div>
        <label>Bio</label>
        <textarea name="bio"></textarea>
    </div>
    <button type="submit">Create</button>
</form>